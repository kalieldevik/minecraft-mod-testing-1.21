{
	"schemaVersion": 1,
	"id": "mcmodtesting",
	"version": "${version}",
	"name": "Minecraft Mod Testing",
	"description": "This is a mod that I will use to learn and test ideas for other mods!",
	"authors": [
		"Kali"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/mcmodtesting/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.kali.mcmodtesting.MinecraftModTesting"
		],
		"client": [
			"net.kali.mcmodtesting.MinecraftModTestingClient"
		],
		"fabric-datagen": [
			"net.kali.mcmodtesting.MinecraftModTestingDataGenerator"
		]
	},
	"mixins": [
		"mcmodtesting.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.21",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}